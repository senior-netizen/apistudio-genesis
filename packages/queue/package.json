{
  "name": "@squirrel/queue",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --out-dir dist",
    "dev": "tsup src/index.ts --watch --format cjs --out-dir dist",
    "lint": "eslint --ext .ts src",
    "postinstall": "yarn run --silent build || echo 'queue build skipped'"
  },
  "dependencies": {
    "bullmq": "^5.29.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
