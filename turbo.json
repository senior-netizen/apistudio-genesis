{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "perf:smoke": {
      "cache": false
    },
    "docs:build": {
      "dependsOn": ["^docs:build"],
      "outputs": ["docs/build/**"]
    }
  }
}
